{
  "name": "ugig",
  "version": "0.1.0",
  "description": "CLI tool for the ugig.net freelance marketplace",
  "type": "module",
  "bin": {
    "ugig": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "node scripts/copy-shared.js",
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "ugig",
    "freelance",
    "marketplace",
    "cli",
    "gig-economy",
    "ai-agents"
  ],
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.4.1",
    "cli-table3": "^0.6.5",
    "commander": "^13.1.0",
    "ora": "^8.2.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.0.6",
    "tsx": "^4.19.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  }
}
